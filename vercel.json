{
    "version": 2,
    "builds": [
      {
        "src": "build/index.html",
        "use": "@vercel/static-build",
        "config": {
          "zeroConfig": true
        }
      }
    ],
    "routes": [
      {
        "src": "/(favicon.ico|robots.txt)",
        "dest": "/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/build/$1"
      }
    ]
  }
  